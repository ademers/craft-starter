{
    "name": "ademers/craft-starter",
    "description": "A Craft CMS starter project by Andrea DeMers.",
    "keywords": [
      "craft",
      "cms",
      "craftcms",
      "project"
    ],
    "type": "project",
    "version": "1.1.3",
    "license": "MIT",
    "require": {
        "craftcms/cms": "3.5.0-RC3",
        "craftcms/contact-form": "2.2.7",
        "craftcms/redactor": "2.6.1",
        "mmikkel/retcon": "2.2.1",
        "nystudio107/craft-minify": "1.2.10",
        "nystudio107/craft-scripts": "1.2.10",
        "nystudio107/craft-seomatic": "3.3.8",
        "nystudio107/craft-twigpack": "1.2.3",
        "putyourlightson/craft-sprig": "1.0.0-beta.9",
        "vlucas/phpdotenv": "^3.4.0"
    },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@composer dump-autoload -o",
      "@php craft setup/welcome"
    ]
  }
}
